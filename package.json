{
  "name": "faine-kino-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index",
  "scripts": {
    "start:dev": "node -r dotenv/config --inspect=0.0.0.0:9227 ./dist/",
    "start": "rimraf dist && npm run lint && copyfiles -e src/email-templates/*.ts -f src/email-templates/*.* dist/email-templates/ && tsc-watch --outDir ./dist --onSuccess \"npm run start:dev\" --onFailure \"'echo Compilation Failed'\"",
    "build": "rimraf dist && tsc",
    "tsc": "rimraf dist && npm run lint && tsc --watch",
    "lint": "eslint --fix --ext .ts,.js src"
  },
  "dependencies": {
    "cors": "2.8.5",
    "dotenv": "16.0.2",
    "express": "4.18.1",
    "express-rate-limit": "6.5.2",
    "helmet": "6.0.0",
    "joi": "17.6.0",
    "jsonwebtoken": "8.5.1",
    "mongoose": "6.5.4",
    "nodemailer": "6.7.8",
    "tslint": "^6.1.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/joi": "^17.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mongoose": "^5.11.97",
    "@types/morgan": "^1.9.3",
    "@types/nodemailer": "^6.4.5",
    "@types/express-rate-limit": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/eslint-plugin-tslint": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "copyfiles": "2.4.1",
    "rimraf": "3.0.2",
    "morgan": "^1.10.0",
    "ts-node": "10.9.1",
    "tsc-watch": "5.0.3",
    "eslint": "8.23.0",
    "typescript": "4.8.2"
  },
